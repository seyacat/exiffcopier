{
  "name": "exifcopy",
  "version": "1.0.0",
  "description": "Electron app to copy metadata between files",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --watch main.js --watch renderer.js --watch index.html --exec \"electron . --dev\"",
    "build": "electron-builder"
  },
  "keywords": ["electron", "metadata", "exif"],
  "author": "User",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "nodemon": "^3.0.0"
  },
  "dependencies": {
    "exifreader": "^4.0.0",
    "exiftool-vendored": "^23.0.0"
  }
}